snippet shade "maven-shade-plugin"
<build>
	<plugins>
		<plugin>
			<groupId>org.apache.maven.plugins</groupId>
			<artifactId>maven-shade-plugin</artifactId>
			<version>3.2.0</version>
			<configuration>
			<!-- put your configurations here -->
			<transformers>
				<transformer implementation="org.apache.maven.plugins.shade.resource.ManifestResourceTransformer">
					<mainClass>${1:path_to_project_entry}</mainClass>
				</transformer>
			</transformers>
			</configuration>
			<executions>
				<execution>
					<phase>package</phase>
					<goals>
					<goal>shade</goal>
					</goals>
				</execution>
			</executions>
		</plugin>
	</plugins>
</build>
endsnippet

snippet exec "maven-exec-plugin"
<build>
	<plugins>
		<plugin>
			<groupId>org.codehaus.mojo</groupId>
			<artifactId>exec-maven-plugin</artifactId>
			<version>1.6.0</version>
			<executions>
				<execution>
					<goals>
						<goal>java</goal>
					</goals>
				</execution>
			</executions>
			<configuration>
				<mainClass>${1:com.example.Main}</mainClass>
				<arguments>
					<argument>${2:argument1}</argument>
				</arguments>
			</configuration>
		</plugin>
	</plugins>
</build>
endsnippet

